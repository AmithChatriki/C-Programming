#include<stdio.h>
#include<string.h>

void str_copy(char dest[],char source[])
{
   int i=0;
   while(source[i]!='\0')
   {
      dest[i]=source[i];
      i++;
   }
   dest[i]='\0';
}

int main()
{
   char str[20],destination[20];
   printf("Enter the String = ");
   scanf(" %s",str);
   str_copy(destination,str);
   printf("Destination String = %s\n",destination);
   
   return 0;
}
